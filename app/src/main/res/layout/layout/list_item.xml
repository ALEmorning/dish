<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="10dp"


        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="ID "/>
            <TextView
                android:id="@+id/iid"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="品名 "/>
            <TextView
                android:id="@+id/iname"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="类别"/>
        <TextView
            android:id="@+id/ileibie"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"

            />
        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="生产许可证号 "/>
        <TextView
            android:id="@+id/ilicense"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"


            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="标准号 "/>
        <TextView
            android:id="@+id/istandard"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="保质期 "/>
        <TextView
            android:id="@+id/idate"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"

            />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="贮存条件 "/>
    <TextView
        android:id="@+id/istore"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"

        />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="制造商 "/>
            <TextView
                android:id="@+id/imake"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="地址 "/>
            <TextView
                android:id="@+id/iaddress"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="产地 "/>
            <TextView
                android:id="@+id/ifrom"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="营养成分 "/>
            <TextView
                android:id="@+id/inutri"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="净含量 "/>
            <TextView
                android:id="@+id/icontent"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="库存 "/>
            <TextView
                android:id="@+id/istock"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"

                />
        </LinearLayout>

    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="10dp"


        />

</LinearLayout>
